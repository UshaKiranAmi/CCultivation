#include <stdio.h>
int main()
{
  //Assignment Operators    =    +=    -=    *=    /=    %=
  
  int a = 10,b = 5;

  printf("Value of a = %d",a);

  a += b; 
  printf("\nValue of a = %d",a);

  a -= b; 
  printf("\nValue of a = %d",a);

  a *= b; 
  printf("\nValue of a = %d",a);

  a /= b; 
  printf("\nValue of a = %d",a);

  a %= b; 
  printf("\nValue of a = %d",a);

  //try editing the program with different values for deeper understanding
  
return 0;
}

